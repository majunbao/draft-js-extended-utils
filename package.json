{
  "name": "draft-js-utils",
  "version": "1.2.5",
  "description": "draft-js utils",
  "main": "build/index.js",
  "scripts": {
    "dev": "watch 'npm run build' src",
    "build": "babel src -d build",
    "test": "mocha --compilers js:babel-core/register --recursive ./tests/*.spec.js",
    "test:watch": "npm run test -- --watch",
    "prepare": "npm run build",
    "release": "np"
  },
  "babel": {
    "presets": [
      "stage-0",
      "es2015"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webdeveloperpr/draft-js-utils.git"
  },
  "keywords": [
    "draft-js",
    "contentState",
    "react",
    "utils"
  ],
  "author": "Luis Betancourt <webdeveloperpr@gmail.com> (https://github.com/webdeveloperpr)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webdeveloperpr/draft-js-utils/issues"
  },
  "homepage": "https://github.com/webdeveloperpr/draft-js-utils#readme",
  "dependencies": {
    "draft-js": "0.10.4"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "chai": "3.5.0",
    "draft-js-raw-content-state": "1.2.5",
    "mocha": "3.4.1",
    "np": "2.16.1",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "watch": "1.0.2"
  }
}
